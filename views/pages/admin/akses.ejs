<div class="main-content">
   <div class="main-content-inner">
      <div class="breadcrumbs ace-save-state" id="breadcrumbs">
         <ul class="breadcrumb">
            <li>
               <i class="ace-icon fa fa-home home-icon"></i>
               <a href="/home">Home</a>
            </li>
            <li class="active">Admin</li>
         </ul>
         <!-- /.breadcrumb -->
         <div class="nav-search" id="nav-search">
            <form class="form-search">
               <span class="input-icon">
               </span>
            </form>
         </div>
         <!-- /.nav-search -->
      </div>
      <div class="page-content">
         <div class="page-header">
            <h1>
               Admin
               <small>
                  <i class="ace-icon fa fa-angle-double-right"></i>
                  Atur Hak Akses
               </small>
            </h1>
         </div>
         <!-- /.page-header -->
         <div class="space-24"></div>
         <!-- ============================== FORM ============== -->

         <% if(messages.info) { %>
         <%- messages.info %>
         <% } %>

         <form method="POST" action="/admin/insert_akses" id="akses-form">
            <div class="col-md-11">
               <div class="row">
                  <!-- ============================== KIRI ============== -->
                  <div class="col-xs-6">
                     <!--  -->
                     <div class="row">
                        <div class="form-group">
                           <div class="col-md-5">
                              <label>NIK : </label>
                           </div>
                           <div class="col-xs-4">
                              <input type="text" name="domain" id="domain" class="form-control" autofocus
                                 data-placement="bottom" data-original-title="Input NIK" minlength="10" maxlength="12"
                                 required onkeypress="return Simbol(event)">
                              <span class="help-block" id="error"></span>
                           </div>
                        </div>
                     </div>

                     <div class="space-2"></div>
                     <div class="row">
                        <div class="form-group">
                           <div class="col-md-5">
                              <label>Nama : </label>
                           </div>
                           <div class="col-md-7">
                              <input type="text" name="nama_karyawan" id="nama_karyawan" class="form-control" autofocus
                                 data-placement="bottom" data-original-title="Nama Karyawan" minlength="3"
                                 maxlength="50" required readonly>
                              <span class="help-block" id="error"></span>
                           </div>
                        </div>
                     </div>

                     <div class="space-2"></div>
                     <div class="row">
                        <div class="form-group">
                           <div class="col-md-5">
                              <label>Cabang : </label>
                           </div>
                           <div class="col-md-7">
                              <select class="default" data-placeholder="Kode Cabang" name="kode_cabang"
                                 id="kode_cabang">
                                 <option value="">- Cabang -</option>
                                 <% data_branch.forEach(function(value) { %>
                                 <option
                                    value="<%= value.OFFICE_CODE %>:<%= value.OFFICE_NAME %>:<%= value.OFFICE_ADDR %>">
                                    <%= value.OFFICE_CODE %> -
                                    <%= value.OFFICE_NAME %></option>
                                 <% }) %>
                              </select>
                              <span class="help-block" id="error"></span>
                           </div>
                        </div>
                     </div>

                     <div id="privilege">
                        <div class="space-2"></div>
                        <div class="row">
                           <div class="form-group">
                              <div class="col-md-5">
                                 <label>Privilege : </label>
                              </div>
                              <div class="col-md-7">
                                 <select name="is_cost_control" id="is_cost_control" class="form-control" required>
                                    <option value=''>- Privilege -</option>
                                    <option value='2'> 2 - All (Cost Control with Master Data Adminstrator) </option>
                                    <option value='1'> 1 - All except Administrator (Cost Control with no Master Data
                                       Administrator</option>
                                    <option value='0'> 0 - None of Above (View Only) </option>
                                 </select>
                                 <span class="help-block" id="error"></span>
                              </div>
                           </div>
                        </div>
                     </div>

                     <div class="space-2"></div>
                     <div class="row">
                        <div class="form-group">
                           <div class="col-md-5">
                              <label>Active : </label>
                           </div>
                           <div class="col-md-7">
                              <select name="active" id="active" class="form-control" required>
                                 <option value=''>- Active -</option>
                                 <option value='1'> 1 - Active</option>
                                 <option value='0'> 0 - Not Active</option>
                              </select>
                              <span class="help-block" id="error"></span>
                           </div>
                        </div>
                     </div>
                     <div class="space-12"></div>
                     <div class="row">
                        <div class="col-md-5">
                           &nbsp;
                        </div>
                        <div class="col-xs-7 text-center">
                           <button type="submit" class="btn btn-primary">Simpan</button>
                        </div>
                     </div>
                  </div>
         </form>
         <div class="row">
            <div class="col-xs-12">
               <h3 class="header smaller lighter blue text-right">
               </h3>
               <div class="clearfix">
                  <div class="pull-right tableTools-container"></div>
               </div>
               <div class="table-header text-center">
                  List User
               </div>
               <!-- div.table-responsive -->
               <!-- div.dataTables_borderWrap -->
               <div class="dt-buttons btn-overlap btn-group btn-overlap">
               </div>
               <div class="table-responsive">
                  <table id="akses-table" class="table table-striped table-bordered table-hover">
                     <thead>
                        <tr>
                           <th>No</th>
                           <th>NIK</th>
                           <th>Nama</th>
                           <th>Cabang</th>
                           <th>Privilege</th>
                           <th>Active</th>
                           <th>Created Date</th>
                           <th>Created By</th>
                           <th>Updated Date</th>
                           <th>Updated By</th>
                           <th>Action</th>
                        </tr>
                     </thead>
                     <tbody>

                        <% var i=1; data.forEach(function(value) { %>
                        <tr>
                           <td>
                              <%- i++ %>
                           </td>
                           <td>
                              <%= value.nik %>
                           </td>
                           <td>
                              <%= value.nama %>
                           </td>
                           <td>
                              <%= value.office_code %>
                           </td>
                           <td>

                              <% if(value.is_cost_control == '2') { %>
                              2 - All (Cost Control with Master Data Adminstrator)
                              <% } else if(value.is_cost_control == '1') { %>
                              1 - All except Administrator (Cost Control with no Master Data
                              Administrator
                              <% } else { %>
                              0 - None of Above (View Only)
                              <% } %>

                           </td>
                           <td>

                              <% if(value.active == '1') { %>
                              1 - Active

                              <% } else { %>
                              0 - Not Active
                              <% } %>
                           </td>
                           <td>
                              <%= value.created_date %>
                           </td>
                           <td>
                              <%= value.created_by %>
                           </td>
                           <td>
                              <%= value.updated_date %>
                           </td>
                           <td>
                              <%= value.updated_by %>
                           </td>
                           <td>
                              <% if(value.nik == 'No Data') { %>
                              <%= 'No Data' %>
                              <% } else { %>
                              <a href="/admin/edit_akses/<%= value.id_akses %>"
                                 onclick="return confirm('Edit data ini?');"><i
                                    class="fa fa-pencil text-success"></i></a>&nbsp;
                              | &nbsp;
                              <a href="/admin/delete_akses/<%= value.id_akses %>"
                                 onclick="return confirm('Hapus data ini?');"><i
                                    class="fa fa-trash text-danger"></i></a></td>
                           <% } %>
                        </tr>
                        <% }); %>

                     </tbody>
                  </table>
               </div>
            </div>
         </div>
      </div>
   </div>
</div>
<!-- /.page-content -->
</div>
</div>
<!-- /.main-content -->